<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="no.hib.dat153.compulsory2">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.LearnActivity" />
        <activity android:name=".activities.ImagesActivity" />
        <activity android:name=".activities.NamesActivity" />
        <activity android:name=".activities.ScoreActivity" />
        <activity android:name=".activities.ShowPersonActivity" />
        <activity android:name=".activities.AddOwnerActivity"></activity>
    </application>

</manifest>
